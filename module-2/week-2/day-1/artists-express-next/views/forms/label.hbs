<form id="form-create-label" method="post" action="/create-label" class="form" enctype="multipart/form-data">

    <h1 class="title">Create new Label</h1>

    <label for="name">name</label>
    <input class="input" id="name" name="name" type="text" required value="test label">

    <label for="city">City</label>
    <input class="input" id="city" name="city" type="text">

    <label for="streetNb">Street number</label>
    <input class="input" id="streetNb" name="streetNb" type="text">

    <label for="street">Street</label>
    <input class="input" id="street" name="street" type="text">

    <label for="country">Country</label>
    <input class="input" id="country" name="country" type="text">

    <label for="logo">Logo</label>
    <input id="logo" type="file" name="logo">

    <button class="btn">create !</button>
</form>